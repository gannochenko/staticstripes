{
  "name": "@gannochenko/staticstripes",
  "version": "0.1.0",
  "description": "Video project renderer",
  "main": "dist/index.js",
  "bin": {
    "staticstripes": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "video",
    "renderer",
    "ffmpeg"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^17.0.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@types/css-tree": "^2.3.11",
    "commander": "^14.0.3",
    "css-tree": "^3.1.0",
    "expr-eval": "^2.0.2",
    "jsdom": "^25.0.1",
    "parse5": "^8.0.0",
    "puppeteer": "^24.36.1"
  }
}
